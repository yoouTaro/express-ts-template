{
   "name": "my-express-template-new",
   "version": "0.0.1",
   "description": "express-ts-template",
   "type": "commonjs",
   "devDependencies": {
      "@types/express": "^4.17.17",
      "@types/node": "^18.15.13",
      "generate-express-crud-file": "^1.0.10",
      "ts-node": "10.7.0",
      "typescript": "4.5.2"
   },
   "dependencies": {
      "@types/socket.io": "^3.0.2",
      "dotenv": "^16.0.3",
      "express": "^4.18.2",
      "nodemon": "^2.0.22",
      "pg": "^8.4.0",
      "reflect-metadata": "^0.1.13",
      "socket.io": "^4.6.1",
      "typeorm": "0.3.15"
   },
   "scripts": {
      "dev": "nodemon -r dotenv/config src/index.ts",
      "start": "ts-node -r dotenv/config src/index.ts",
      "build": "tsc",
      "typeorm": "typeorm-ts-node-commonjs",
      "generate": "yarn generate-express-crud-file",
      "migration:generate": "yarn typeorm -- -d ./src/database/data-source.ts migration:generate",
      "migration:run": "yarn typeorm migration:run -- -d ./src/database/data-source.ts",
      "migration:create": "yarn typeorm -- migration:create",
      "migration:revert": "yarn typeorm -- -d ./src/database/data-source.ts migration:revert"
   }
}